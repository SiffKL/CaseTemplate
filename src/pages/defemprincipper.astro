---
import Fiveprinciplessection from "../components/campaignsite2/Fiveprinciplessection.astro";
import Newsletterform from "../components/campaignsite2/Newsletterform.astro";
import VersusSection from "../components/campaignsite2/VersusSection.astro";
import BaseLayout from "../layouts/BaseLayout.astro";
import Title from "../components/globalcomponents/Title.astro";
import Faktabox from "../components/campaignsite2/Faktabox.astro";
---

<BaseLayout>
  <Title
    manchet="Som plastic changer har vi"
    bigred="5 principper"
    text="Alle, der er med til at sætte fokus på plastik dagsordenen er i princippet en Plastic Changer. Vi drives af ønsket om at skabe en positiv forandring i retningen mod ansvarlig brug af plastik."
  />
  <Fiveprinciplessection />
  <Newsletterform />
  <Faktabox />
  <VersusSection />
  <img
    style="display: block;-webkit-user-select: none;margin: auto;"
    src="https://shopify.vivre-motion.com/thumbnail_icons/teams/2000_spouting-whale_1f433.png"
  />
</BaseLayout>
